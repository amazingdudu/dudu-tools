{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require": 0,
        "no-shadow": 0,
        "no-param-reassign": 0,
        "import/no-dynamic-require": 0
    }
}
